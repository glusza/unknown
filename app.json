{
  "expo": {
    "name": "Unknown",
    "slug": "unknown",
    "version": "0.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/dark-icon.png",
    "scheme": "unknown",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "buildNumber": "2",
      "supportsTablet": false,
      "bundleIdentifier": "com.unknown.band",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["audio"],
        "NSMicrophoneUsageDescription": "This app uses audio playback features"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dark-icon.png",
        "backgroundColor": "#19161a"
      },
      "package": "com.unknown.band",
      "permissions": [
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "intentFilters": [
        {
          "action": "android.media.action.MEDIA_BUTTON",
          "category": ["android.intent.category.DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/dark-icon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "expo-av"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a721615a-bdf2-41a9-8df7-3efb21fb5eb9"
      }
    }
  }
}
